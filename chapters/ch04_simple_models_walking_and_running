        "from pydrake.all import (\n",
        "    ConstantVectorSource,\n",
        "    DiagramBuilder,\n",
        "    LogVectorOutput,\n",
        "    MeshcatVisualizer,\n",
        "    SceneGraph,\n",
        "    Simulator,\n",
        "    StartMeshcat,\n",
        ")\n",
        "from pydrake.examples import CompassGait, CompassGaitGeometry\n",
        "\n",
        "from underactuated import running_as_notebook\n",
        "\n"
      ],
      "metadata": {
        "collapsed": true,
        "cellView": "form",
